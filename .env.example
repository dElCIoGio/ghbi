# Environment configuration example
# Copy this file to `.env` and fill in the values for your Shopify store

# The current environment (e.g. development, production)
VITE_ENV=development

# SHOPIFY
# Domain of your Shopify store without protocol
VITE_SHOPIFY_DOMAIN="your-store.myshopify.com"

# Storefront access token for your Shopify store
VITE_SHOPIFY_STOREFRONT_TOKEN="your-storefront-token"

# Shopify API version to use
VITE_SHOPIFY_API_VERSION="2025-04"
